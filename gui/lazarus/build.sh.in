#!/bin/sh
BUILD_PROJECT=@sh_project@
BUILD_TARGET_OS=@sh_os@
BUILD_TARGET_WS=@sh_ws@
BUILD_TARGET_CPU=@sh_arch@

BUILD(){
    if [ -f $BUILD_PROJECT ]
    then
        # build application
        echo "--cpu=$BUILD_TARGET_CPU --operating-system=$BUILD_TARGET_OS --ws=$BUILD_TARGET_WS  $BUILD_PROJECT"
        lazbuild --cpu=$BUILD_TARGET_CPU --operating-system=$BUILD_TARGET_OS --ws=$BUILD_TARGET_WS  $BUILD_PROJECT
    else
        echo "Don't found this lazarus project"
    fi
}

BUILD
